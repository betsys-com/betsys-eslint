{
    "extends": "nx/presets/npm.json",
    "$schema": "./node_modules/nx/schemas/nx-schema.json",
    "tasksRunnerOptions": {
        "default": {
            "options": {
                "canTrackAnalytics": false,
                "showUsageWarnings": true
            }
        }
    },
    "affected": {
        "defaultBase": "main"
    },
    "workspaceLayout": {
        "libsDir": "packages"
    },
    "targetDefaults": {
        "build": {
            "cache": true
        },
        "lint": {
            "cache": true
        },
        "test": {
            "cache": true
        }
    }
}
