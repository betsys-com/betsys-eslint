# Editor configuration, see http://editorconfig.org
root = true

[*]
charset = utf-8
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.ts]
max_line_length = 140

[*.js]
max_line_length = 140

[*.md]
max_line_length = off
trim_trailing_whitespace = false
