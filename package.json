        "eslint-plugin-no-autofix": "^1.2.3",
        "lerna": "^8.0.1",
        "nx": "17.2.6",
        "nx-cloud": "16.5.2",
        "prettier": "^2.6.2",
        "rollup": "^2.79.0",
        "rollup-plugin-terser": "^7.0.2",
